{
  "name": "@sorgan-components/core",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/sorgan-components.umd.js",
  "module": "./dist/sorgan-components.es.js",
  "types": "./dist/sorgan-components.es.d.ts",
  "exports": {
    ".": {
      "import": "./dist/sorgan-components.es.js",
      "require": "./dist/sorgan-components.umd.js"
    },
